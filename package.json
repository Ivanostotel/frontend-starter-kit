{
  "name": "frontend-starter-kit",
  "description": "Modern frontend only architecture with gulp, zurb panini, postcss and babel.",
  "version": "3.0.0",
  "author": "Rumen Petrov",
  "license": "MIT",
  "main": "gulpfile.babel.js",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 10"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-modules-commonjs",
        {
          "allowTopLevelThis": true
        }
      ]
    ]
  },
  "scripts": {
    "start": "gulp",
    "assets": "gulp assets",
    "build": "gulp build"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "autoprefixer": "^9.5.1",
    "del": "^4.1.0",
    "doiuse": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.1",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.3",
    "gulp-eslint": "^5.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "panini": "^1.6.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.6.0"
  }
}
